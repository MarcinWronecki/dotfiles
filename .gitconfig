[push]
	autoSetupRemote = true
[alias]
	co = checkout
	st = status
[user]
	email = 2470703+MarcinWronecki@users.noreply.github.com
	name = Marcin Wronecki
[core]
	pager = delta
[init]
	defaultBranch = master
[help]
	autoCorrect = immediate
[delta]
	# https://dandavison.github.io/delta/configuration.html
	hyperlinks = true
	line-numbers = true
	navigate = true
	syntax-theme = Monokai Extended
[diff]
	colorMoved = default
[interactive]
	diffFilter = delta --color-only
[merge]
	conflictStyle = zdiff3
